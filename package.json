{
  "name": "proposal-flow",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "homepage": "https://thrare00.github.io/proposal-flow",
  "scripts": {
    "dev": "vite",
    "build": "npm run prebuild && npm run postbuild",
    "preview": "vite preview",
    "build:clean": "rimraf dist && npm run build",
    "prebuild": "tsc --noEmit && vite build",
    "postbuild": "copyfiles -u 1 public/* dist/ && cp -r public/* dist/ && echo proposal-flow.thrare00.github.io > dist/CNAME"
  },
